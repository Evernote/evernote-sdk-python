#: admin.py:12
msgid "Content"
msgstr "Lëndë"

#: admin.py:15
msgid "Metadata"
msgstr "Tejtëdhëna"

#: admin.py:42
msgid "flagged"
msgid_plural "flagged"
msgstr[0] "me shenjë"
msgstr[1] "me shenjë"

#: admin.py:43
msgid "Flag selected comments"
msgstr "Vëru shenjë komenteve të përzgjedhura"

#: admin.py:47
msgid "approved"
msgid_plural "approved"
msgstr[0] "i miratuar"
msgstr[1] "të miratuar"

#: admin.py:48
