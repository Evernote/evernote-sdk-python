msgid "user"
msgstr "käyttäjä"

#: models.py:52
msgid "user's name"
msgstr "käyttäjän nimi"

#: models.py:53
msgid "user's email address"
msgstr "käyttäjän sähköpostiosoite"

#: models.py:54
msgid "user's URL"
msgstr "käyttäjän URL"

#: models.py:56 models.py:76 models.py:169
msgid "comment"
msgstr "kommentti"

#: models.py:59
msgid "date/time submitted"
msgstr "lähettämishetki"

