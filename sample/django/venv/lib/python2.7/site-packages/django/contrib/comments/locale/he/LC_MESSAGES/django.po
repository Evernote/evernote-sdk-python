msgid "user"
msgstr "משתמש"

#: models.py:52
msgid "user's name"
msgstr "שם משתמש"

#: models.py:53
msgid "user's email address"
msgstr "כתובת דוא\"ל משתמש"

#: models.py:54
msgid "user's URL"
msgstr "אתר המשתמש"

